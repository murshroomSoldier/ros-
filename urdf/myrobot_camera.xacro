  </collision>
</link>

<link name="right_wheel_1">
  <inertial>
    <origin xyz="-5.790023100122029e-18 -0.012499999999999997 0.0" rpy="0 0 0"/>
    <mass value="0.2797980957103409"/>
    <inertia ixx="0.000189" iyy="0.00035" izz="0.000189" ixy="-0.0" iyz="-0.0" ixz="0.0"/>
